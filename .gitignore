# Ignore bundler config.
/rails_app/.bundle

# Ignore all logfiles and tempfiles.
/rails_app/log/*
/rails_app/tmp/*
!/rails_app/log/.keep
!/rails_app/tmp/.keep

# Ignore Byebug command history file.
/rails_app/.byebug_history
/rails_app/docker-compose.dev.yml

elm-app/node_modules
elm-app/elm-stuff
elm-app/dist